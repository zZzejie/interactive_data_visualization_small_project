{"ast":null,"code":"import ScatterPlot from './ScatterPlot';\nimport LinePlot from './LinePlot';\nimport BarChart from './BarChart'; // Import the new component\n\nexport default {\n  components: {\n    ScatterPlot,\n    LinePlot,\n    BarChart\n  },\n  data: () => ({\n    scatterPlotId: 0,\n    linePlotId: 0,\n    BarChartId: 0,\n    // Add a new data property to store the BarChart ID\n    averageEmployeeCount: 0,\n    // Add a new data property to store the average employee count\n\n    categories: {\n      values: ['All', 'tech', 'health', 'bank'],\n      selectedValue: 'All'\n    },\n    companies: {\n      values: ['alphabet', 'apple', 'amazon', 'microsoft', 'meta', 'united health', 'johnson and johnson', 'pfizer', 'cvs health', 'mckesson', 'ubs', 'credit suisse', 'jp morgan', 'goldman sachs', 'bank of america'],\n      selectedValue: 'alphabet'\n    },\n    algorithm: {\n      values: ['none', 'random', 'regression'],\n      selectedValue: 'none'\n    }\n  }),\n  methods: {\n    changeCategory() {\n      this.scatterPlotId += 1;\n    },\n    changeCompany() {\n      this.linePlotId += 1;\n    },\n    changeAlgorithm() {\n      this.linePlotId += 1;\n    },\n    changeCurrentlySelectedCompany(companyName) {\n      this.companies.selectedValue = companyName;\n      this.changeCompany();\n    },\n    changeAverageEmployeeCount(averageEmployeeCount) {\n      this.averageEmployeeCount = averageEmployeeCount;\n      this.BarChartId += 1;\n    }\n  }\n};","map":{"version":3,"names":["ScatterPlot","LinePlot","BarChart","components","data","scatterPlotId","linePlotId","BarChartId","averageEmployeeCount","categories","values","selectedValue","companies","algorithm","methods","changeCategory","changeCompany","changeAlgorithm","changeCurrentlySelectedCompany","companyName","changeAverageEmployeeCount"],"sources":["/Users/zoe/Desktop/example-project/hello-world/services/frontend/src/components/ConfigurationPanel.vue"],"sourcesContent":["<template>\n  <div class=\"mainContainer\">\n    <v-container fluid class=\"controlPanelContainer\">\n      <v-row>\n        <v-col cols=\"12\" md=\"2\" class=\"sideBar\">\n          <v-row>\n            <v-col cols=\"12\" sm=\"12\">\n              <div class=\"control-panel-font\">Company Overview</div>\n            </v-col>\n          </v-row>\n          <v-row>\n            <v-col cols=\"12\" sm=\"12\">\n              <v-select\n                  :items=\"categories.values\"\n                  label=\"Select a category\"\n                  dense\n                  v-model=\"categories.selectedValue\"\n                  @change=\"changeCategory\"\n                  color=\"rgba(20, 164, 77, 1)\"\n              ></v-select>\n            </v-col>\n          </v-row>\n          <v-row>\n            <v-col cols=\"12\" sm=\"12\">\n              <div class=\"control-panel-font\">Profit View of Company</div>\n            </v-col>\n          </v-row>\n          <v-row>\n            <v-col cols=\"12\" sm=\"12\">\n              <v-select\n                  :items=\"companies.values\"\n                  label=\"Select a company\"\n                  dense\n                  v-model=\"companies.selectedValue\"\n                  @change=\"changeCompany\"\n                  color=\"rgba(20, 164, 77, 1)\"\n              ></v-select>\n            </v-col>\n          </v-row>\n          <v-row>\n            <v-col cols=\"12\" sm=\"12\">\n              <v-select\n                  :items=\"algorithm.values\"\n                  label=\"Select an algorithm\"\n                  dense\n                  v-model=\"algorithm.selectedValue\"\n                  @change=\"changeAlgorithm\"\n                  color=\"rgba(20, 164, 77, 1)\"\n              ></v-select>\n            </v-col>\n          </v-row>\n          <v-row>\n            <v-col cols=\"12\" sm=\"12\">\n              <div class=\"control-panel-font\">Employee View of Company</div>\n            </v-col>\n          </v-row>\n          <v-row>\n            <v-col cols=\"12\" sm=\"12\">\n              <v-select\n                  :items=\"categories.values\"\n                  label=\"Select a category\"\n                  dense\n                  v-model=\"categories.selectedValue\"\n                  @change=\"changeCategory\"\n                  color=\"rgba(20, 164, 77, 1)\"\n              ></v-select>\n            </v-col>\n          </v-row>\n        </v-col>\n\n        <v-col cols=\"12\" md=\"5\">\n          <!-- Allow an emitted event from the child component (Company Overview) to the parent component (Configuration Panel).-->\n          <ScatterPlot :key=\"scatterPlotId\"\n                       :selectedCategory=\"categories.selectedValue\"\n                       @changeCurrentlySelectedCompany=\"changeCurrentlySelectedCompany\"\n          />\n          <BarChart :key=\"BarChartId\"\n                    :selectedCategory=\"categories.selectedValue\"\n                    :selectedCompany=\"companies.selectedValue\"\n                    :averageEmployeeCount=\"averageEmployeeCount\"\n                    @changeAverageEmployeeCount=\"changeAverageEmployeeCount\"\n          />\n        </v-col>\n        <v-col cols=\"12\" md=\"5\">\n          <LinePlot :key=\"linePlotId\"\n                    :selectedCompany=\"companies.selectedValue\"\n                    :selectedAlgorithm=\"algorithm.selectedValue\"/>\n        </v-col>\n        <!-- Add a Bar chart to visualize the employee comparison -->\n      </v-row>\n    </v-container>\n  </div>\n</template>\n<script>\nimport ScatterPlot from './ScatterPlot';\nimport LinePlot from './LinePlot';\nimport BarChart from './BarChart'; // Import the new component\n\nexport default {\n  components: {ScatterPlot, LinePlot, BarChart},\n  data: () => ({\n    scatterPlotId: 0,\n    linePlotId: 0,\n    BarChartId: 0, // Add a new data property to store the BarChart ID\n    averageEmployeeCount: 0, // Add a new data property to store the average employee count\n\n    categories: {\n      values: ['All', 'tech', 'health', 'bank'],\n      selectedValue: 'All'\n    },\n    companies: {\n      values: ['alphabet', 'apple', 'amazon', 'microsoft', 'meta', 'united health', 'johnson and johnson', 'pfizer', 'cvs health', 'mckesson', 'ubs', 'credit suisse', 'jp morgan', 'goldman sachs', 'bank of america'],\n      selectedValue: 'alphabet'\n    },\n    algorithm: {\n      values: ['none', 'random', 'regression'],\n      selectedValue: 'none'\n    },\n  }),\n  methods: {\n    changeCategory() {\n      this.scatterPlotId += 1\n    },\n    changeCompany() {\n      this.linePlotId += 1\n    },\n    changeAlgorithm() {\n      this.linePlotId += 1\n    },\n    changeCurrentlySelectedCompany(companyName) {\n      this.companies.selectedValue = companyName\n      this.changeCompany()\n    },\n    changeAverageEmployeeCount(averageEmployeeCount) {\n      this.averageEmployeeCount = averageEmployeeCount\n      this.BarChartId += 1\n    }\n  },\n\n  }\n</script>\n\n\n<style scoped>\n.mainContainer {\n  background: rgba(20, 164, 77, .05);\n}\n\n.control-panel-font {\n  font-family: \"Open Sans\", verdana, arial, sans-serif;\n  align-items: center;\n  font-size: 15px;\n  //border-bottom: 1px solid rgba(0, 0, 0, 0.1);\n  border-bottom: 1px solid rgba(20, 164, 77, .75);\n  display: flex;\n  font-weight: 500;\n  height: 40px;\n}\n.sideBar {\n  //border-right: 1px solid rgba(0, 0, 0, 0.1);\n  border-right: 1px solid rgba(20, 164, 77, .75);\n  //background: #fafafa;\n  background: rgba(20, 164, 77, .05);\n  padding-left: 17px;\n  height: calc(100vh - 50px);\n}\n</style>\n"],"mappings":"AA8FA,OAAOA,WAAU,MAAO,eAAe;AACvC,OAAOC,QAAO,MAAO,YAAY;AACjC,OAAOC,QAAO,MAAO,YAAY,EAAE;;AAEnC,eAAe;EACbC,UAAU,EAAE;IAACH,WAAW;IAAEC,QAAQ;IAAEC;EAAQ,CAAC;EAC7CE,IAAI,EAAEA,CAAA,MAAO;IACXC,aAAa,EAAE,CAAC;IAChBC,UAAU,EAAE,CAAC;IACbC,UAAU,EAAE,CAAC;IAAE;IACfC,oBAAoB,EAAE,CAAC;IAAE;;IAEzBC,UAAU,EAAE;MACVC,MAAM,EAAE,CAAC,KAAK,EAAE,MAAM,EAAE,QAAQ,EAAE,MAAM,CAAC;MACzCC,aAAa,EAAE;IACjB,CAAC;IACDC,SAAS,EAAE;